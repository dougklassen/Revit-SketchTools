<Window x:Class="BCRA.Revit.QueryDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Search Text" Height="182" Width="552">
    <Canvas>
        <Button Canvas.Left="354" Canvas.Top="99" Content="Report" Height="23" Name="reportButton" Width="75" Click="searchButton_Click" />
        <Button Canvas.Left="443" Canvas.Top="99" Content="Cancel" Height="23" Name="cancelButton" Width="75" Click="cancelButton_Click" />
        <Label Canvas.Left="12" Canvas.Top="12" Content="Search for text:" Height="28" Name="label1" />
        <TextBox Canvas.Left="12" Canvas.Top="46" Height="29" Name="queryTextBox" Width="506" />
        <Button Canvas.Left="262" Canvas.Top="99" Content="Flag" Height="23" Name="flagButton" Width="75" Click="flagButton_Click" />
    </Canvas>
</Window>
